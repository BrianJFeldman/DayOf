{
    "manifest_version": 2,
    "name": "DayOf",
    "description": "This extension will link to your calendar for any date you highlight",
    "version": "1.0",
    "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
    "browser_action": {
        "default_popup": "dayOf.html",
        "default_icon":"icon128.png"
    },
    "content_scripts": [
        {
            "js": [
                "dayOf.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}